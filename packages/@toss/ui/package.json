    "@storybook/react-vite": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-storybook": "^0.6.13",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.3",
